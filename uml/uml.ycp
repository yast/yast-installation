{
    textdomain "installation";

    import "Mode";
    import "Wizard";

    // set normal mode and update
    // Mode::normal = true;
    // Mode::installation = false;
    // Mode::update = false;

    // wizard stage
    integer current_stage = -1;

    // create initial dialog
    Wizard::CreateDialog ();

    // calling inst_proposal
    symbol ret = (symbol) WFM::CallFunction ("inst_proposal", [true, true, `uml]);

    if (ret == `next)
    {
	WFM::CallFunction ("inst_uml_kickoff", [false, false]);
    }

    Wizard::CloseDialog ();

    return `next;
}
