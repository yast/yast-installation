        <workflow>
            <defaults>
                <archs>all</archs>
            </defaults>
            <stage>initial</stage>
	    <label>Base Installation</label>
            <mode>demo</mode>
            <modules  config:type="list">
                <module>
                    <enable_back>no</enable_back>
