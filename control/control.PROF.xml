                    <name>do_resize</name>
                    <update config:type="boolean">false</update>
                    <archs>i386,x86_64,ia64</archs>
                </module>
                <module>
 	            <label>Perform Installation</label>
                   <name>prepdisk</name>
                </module>
                <module>
	            <label>Perform Installation</label>
                    <name>kickoff</name>
                </module>
                <module>
	            <label>Perform Installation</label>
                    <name>rpmcopy</name>
                </module>
            </modules>
