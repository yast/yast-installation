                    <enable_next>yes</enable_next>
                </module>
		<module>
		    <heading>yes</heading>
		    <label>Update</label>
		</module>
                <module>
	            <label>Update Summary</label>
                    <name>proposal</name>
                    <proposal>initial</proposal>
                </module>
                <module>
	            <label>Perform Update</label>
                    <name>do_resize</name>
                    <update config:type="boolean">false</update>
                    <archs>i386,x86_64,ia64</archs>
                </module>
                <module>
	            <label>Perform Update</label>
