      <ref name="LIST"/>
      <oneOrMore>
        <element name="extra_url">
          <interleave>
            <element name="baseurl">
              <text/>
            </element>
            <optional>
              <element name="name">
                <text/>
              </element>
            </optional>
            <optional>
              <element name="alias">
                <text/>
              </element>
            </optional>
            <optional>
              <element name="prod_dir">
                <text/>
              </element>
            </optional>
            <optional>
              <element name="enabled">
